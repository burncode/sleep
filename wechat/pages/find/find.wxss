/* pages/find/find.wxss */
/*外层用column,代表每个列表*/

.item-container{
  display: flex;  
  flex-direction: column;
}
/*中层用row,代表头像和其他信息在同一行*/
.item-row{
  display:flex;
  flex-direction: row;
}
/*内层用column，即其他信息在同一列*/
.item-column-left{
  display:flex;
  flex-direction: column;
  margin: 20rpx;
}
.item-column-right{
  display:flex;
  flex-direction: column;
  margin: 20rpx;
  /*加下面一行，就可以修复两端对齐的bug了*/
  width: 100%
}

.item-avatar{
  height: 80rpx;
  width: 80rpx;
  border-radius: 5%
}
.item-nickName{
  font-size: 30rpx;
  color: #BCC2EA;
}
.item-word{
  font-size: 30rpx;
    color: #747DB1
}
.item-dateTimeLike-out{
  display:flex;
  flex-direction: column;
}
.item-dateTimeLike{
  display:flex;
  flex-direction: row;
  justify-content: space-between;
  color: #747DB1;
  font-size: 25rpx;
}
.like{
 /* margin-left: 220rpx;*/
   font-size: 28rpx;
}
.like_pic{
  height: 28rpx;
  width: 28rpx;
}
.item-line{
width: 100%;
border-top:1rpx #292D48 solid;
}
.bottom{
  color: #434A76;
  font-size: 30rpx;
  display:flex;
  justify-content: center;
}
/*修饰form中的button*/
button.form_button{background-color:transparent;padding:0;margin:0;display:inline;position:static;border:0;padding-left:0;padding-right:0;border-radius:0;font-size:0rpx;color:transparent;}
button.form_button::after{content:'';width:0;height:0;-webkit-transform:scale(1);transform:scale(1);display:none;background-color:transparent;}
/*  加载更多   */
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
  color:#BCC2EA;
}

